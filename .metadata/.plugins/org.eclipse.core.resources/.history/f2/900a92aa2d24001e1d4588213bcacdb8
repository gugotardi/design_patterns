package strategies;

public class CreditCard {
	private int amount;
	private String number;
	private String date;
	private String cvv;
	public CreditCard(int amount, String number, String date, String cvv) {
		super();
		this.amount = 100_000;
		this.number = number;
		this.date = date;
		this.cvv = cvv;
	}
	
	

}
