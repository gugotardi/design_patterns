package Singleton;

public class Singleton {
	
	private static Singleton instance;
	 public String value;

	private Singleton(String valuec) {
		
		try {
            Thread.sleep(1000);
        } catch (InterruptedException ex) {
            ex.printStackTrace();
        }
        this.value = value;
	}

	public static Singleton getInstance(String Value) {
		if (instance == null) {
            instance = new Singleton(value);
        }
        return instance;
	}
	

}
