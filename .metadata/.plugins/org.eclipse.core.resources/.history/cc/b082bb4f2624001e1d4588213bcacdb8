package Square;

public class SquarePeg {
	private double radius;
	
	public SquarePeg(double radius) {
		this.radius = radius;
	}

	public double getRadius() {
		return radius;
	}

	public boolean fits(RoundPeg peg) {
		boolean result;
		result =(this.getRadius()>= peg.getRadius());
		return result;
	}
	

}
